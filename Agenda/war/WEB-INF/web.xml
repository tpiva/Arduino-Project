<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
	<servlet>
		<servlet-name>contatoBusiness</servlet-name>
		<servlet-class>com.pos.agenda.server.ContatoBusiness</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>contatoBusiness</servlet-name>
		<url-pattern>/agenda/listar</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>contatoBusiness</servlet-name>
		<url-pattern>/agenda/adicionarContato</url-pattern>
	</servlet-mapping>
</web-app>
